- name: <%= branch_name.gsub('/', '-') %>-git-src
  type: git
  source:
    uri: <%= uri %>
    branch: <%= branch_name %>
    private_key: {{githubPrivateKey}}

- name: <%= branch_name %>-repo-status
  type: github-status
  source:
    repository: dennispidun/photobox
    access_token: {{githubToken}}
    branch: <%= branch_name %>
    context: concourse-ci